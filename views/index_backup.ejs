<script type="text/javascript">
    $(window).load(function() {
        new WOW().init();

        // initialise flexslider
        $('.flexslider').flexslider({
            animation: "fade",
            directionNav: true,
            controlNav: false,
            keyboardNav: true,
            slideToStart: 0,
            animationLoop: true,
            pauseOnHover: false,
            slideshowSpeed: 4000,
        });

        smoothScroll.init();

        // initialize isotope
        var $container = $('.portfolio_container');
        $container.isotope({
            filter: '*',
        });

        $('.portfolio_filter a').click(function(){
            $('.portfolio_filter .active').removeClass('active');
            $(this).addClass('active');

            var selector = $(this).attr('data-filter');
            $container.isotope({
                filter: selector,
                animationOptions: {
                    duration: 500,
                    animationEngine : "jquery"
                }
            });
            return false;
        });
    });


    var data = new Array();
    $.ajax({
        type:'GET',
        url: 'users/getPhotographyUrl',
        success: (response)=>{
            data = response;
            console.log(data[0].url);
            let insert = '';
            insert += "<div class=\"col-md-4 photography\">";
            insert += "<a href=\"single-project.html\" class=\"portfolio_item work-grid\">";
            insert += "<img src=\"" + data[0].url + "\" alt=\"image\">";
            insert += "<div class=\"portfolio_item_hover\">";
            insert += "<div class=\"item_info\">";
            insert += "<span>Brave man</span>";
            insert += "<span>Brave man</span>";
            insert += "<em>photography</em>";
            insert += "</div>";
            insert += "</div>";
            insert += "</a>";
            insert += " </div;>";
            console.log(insert);
            $('.row.portfolio_container').after(insert);
            //$(insert).insertAfter('.row portfolio_container');

        }

    });
    console.log(data);

</script>
